Package: jrc
Type: Package
Title: Exchange Commands Between R and 'JavaScript'
Version: 0.5.0
Date: 2021-09-13
Authors@R: c(
	person("Svetlana", "Ovchinnikova", role = c("aut", "cre"), email = "s.ovchinnikova@zmbh.uni-heidelberg.de"),
	person("Simon", "Anders", role = c("aut"), email = "sanders@fs.tum.de")
